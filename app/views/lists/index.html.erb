<div id="banner">
  <h1>WATCH LIST</h1>
  <a href="#"><%= link_to 'new list', lists_new_path %></a>
</div>
<% @lists.each do |list| %>
  <div class="card">
    <% if list.photo.attached? %>
      <a href="#"><%= cl_image_tag list.photo.key, height: 150, width: 200, crop: :fill %></a>
    <% end %>
    <h1><%= link_to list.name.upcase, list_path(list) %></h1>
    <button><%= link_to 'Delete', delete_list_path(list) %></button>
  </div>
<% end %>
<br>
